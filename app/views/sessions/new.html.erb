<br><br><br>
<div id="main">
<h1>Sign in</h1>
<%= form_with(scope: :session, url: sessions_path, local: true) do |f| %>

  <%= f.email_field :email,placeholder: "Email",:class=>'name' %><br><br>

  <%= f.password_field :password, placeholder: "Password",:class=>'name' %><br><br>

  <%= f.submit "Log in",:class=>'submit' %>
<% end %>
</div>
